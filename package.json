{
  "name": "aws-devtools",
  "description": "scripts to be used with CDK or as in a standalone CLI mode",
  "version": "0.1.0",
  "scripts": {
    "deploy:site:s3": "node devtools/s3-copy-to-website-bucket",
    "deploy:server:ecs": "node devtools/update-ecs-service",
    "build:site:s3": "rimraf actions/deploy-s3-site/dist && parcel build ./actions/deploy-s3-site/index.ts --dist-dir actions/deploy-s3-site/dist --no-autoinstall --target default --no-source-maps"
  },
  "targets": {
    "default": {
      "includeNodeModules": true
    }
  },
  "engines": {
    "node": "16"
  },
  "devDependencies": {
    "@types/jest": "26.0.10",
    "@types/node": "10.17.27",
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "5.14.0",
    "eslint": "8.11.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "26.4.2",
    "rimraf": "3.0.2",
    "prettier": "2.5.1",
    "typescript": "4.7.3"
  },
  "dependencies": {
    "@actions/core": "1.8.2",
    "@aws-sdk/credential-provider-sso": "3.56.0",
    "@types/fs-extra": "9.0.13",
    "@types/lodash": "4.14.178",
    "aws-sdk": "2.1100.0",
    "commander": "9.1.0",
    "fs-extra": "10.0.1",
    "ksuid": "3.0.0",
    "lodash": "4.17.21",
    "mime-types": "2.1.35",
    "parcel": "2.6.0"
  }
}
